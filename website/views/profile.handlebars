<div class="container1">
     <center><h3>Your Profile</h3></center>
        <center>
                <div class="content">
                   <div class="primg">
                       <a href="/users/profile/{{data.enrollment}}"+><img class="img1" src="/users/profile/{{data.enrollment}}" alt="Choose Profile" style="width: 100px;height:100px;"></a>
                       <form method="post" enctype="multipart/form-data" action="/users/upload">
                            <input style="width:200px;" type="file" name="file" required>
                            <input type="submit" value="Submit" style="background-color:transparent;border:1px solid black;">
                        </form>
                   </div>
                   <br>
                   <br>
                   <br>
                   <div class="profile">
                       <div class="name">
                         
                          <p class="i">Name :</p>
                          <p class="ii">{{data.name}}</p>
                       </div>
                       <div class="email">
                       
                           <p class="i">Email :</p>
                          <p class="ii">{{data.email}}</p>
                       </div>
                       <div class="email">
                         
                           <p class="i">Mobile :</p>
                          <p class="ii">{{data.mobile}}</p>
                       </div>
                       <div class="email">
                          
                           <p class="i">Enr No:</p>
                          <p class="ii">{{data.enrollment}}</p>
                       </div>
                       <div class="email">
                         
                           <p class="i">Semester</p>
                        
                           <p class="ii">{{data.semester}}</p>
                       </div>
                        <div class="email">
                          
                           <p class="i">Department</p>
                           
                           <p class="ii">{{data.department}}</p>
                       </div>
                       <div class="email">
                      
                           <p class="i">Gender:</p> 
                           <p class="ii">{{data.gender}}</p>
                       </div>
                   </div>
                </div>
                <div class="buttu">
                       <button class="butt" onclick="location.href='/users/edit_profile'">Edit Profile
                       </button>
                       <button class="butt" onclick="location.href='/users/change_password'">Change Password
                       </button>
                   </div>
        </center>
    </div>